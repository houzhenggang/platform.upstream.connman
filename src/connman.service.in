[Unit]
Description=Connection service
Requires=dbus.socket
After=dbus.socket
Before=remote-fs.target

[Service]
EnvironmentFile=-/etc/sysconfig/connman
Type=dbus
BusName=net.connman
Restart=on-failure
ExecStart=@prefix@/sbin/connmand -n $OPTIONS
StandardOutput=null

[Install]
WantedBy=multi-user.target
